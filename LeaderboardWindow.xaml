<Window x:Class="TestTaskGF.LeaderboardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestTaskGF"
        mc:Ignorable="d"
        Title="LeaderboardWindow" Closing="LeaderboardWindow_Closing" Loaded="LeaderboardWindow_Loaded" Height="450" Width="800">
    <Grid>
        <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
            <StackPanel x:Name="leaderboardContainer" Orientation="Vertical" Margin="100 50"/>
        </ScrollViewer>

    </Grid>
</Window>
